    "@typescript-eslint/parser": "^5.37.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-plugin-security": "^1.5.0",
    "feathers-fletching": "^1.20.5",
    "feathers-knex": "^8.0.1",
    "feathers-memory": "^4.1.0",
    "feathers-mongodb": "^6.4.1",
    "feathers-mongoose": "^8.5.1",
    "feathers-nedb": "^6.0.0",
